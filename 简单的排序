import java.util.*;

public class jiandanpaixu {
	public static void main(String[] args) {
		Scanner reader = new Scanner(System.in);
		int n = reader.nextInt();
		int[] a = new int[n];//定义一个数组a
		for(int i=0;i<n; i++) {
			a[i] = reader.nextInt();
		}//读入数组
		for(int i=0; i<n; i++) {
			for(int j=i; j<n; j++) {
				if(a[j]<a[i]) {
					int temp;
					temp = a[j];
					a[j] = a[i];
					a[i] = temp;
				}
			}
		}
		for(int i=0; i<n; i++) {
			System.out.print(a[i] + " ");
		}
	}
}
